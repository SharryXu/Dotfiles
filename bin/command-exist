#!/usr/bin/env bash

# Check command existed or not.
# Globals:
#   command
# Arguments:
#   Command Name
# Returns:
#   Command Exist Status

function usage() {
  echo -e "usage: $(basename "$0") command_name"

  exit 1
}

if [ $# -eq 1 ]; then
	if command -v "$1" >/dev/null 2>&1; then
    exit 0
	else
    exit 1
  fi
else
  usage
fi
