#!/usr/bin/env bash

# Check command existed or not.
# Globals:
#   command
# Arguments:
#   Command Name
# Returns:
#   Command Exist Status

function usage() {
  echo -e "usage: $(basename $0) command_name"
}

if [ $# -eq 1 ]; then
	if command -v $1 >/dev/null 2>&1; then
		echo 0
	else
    echo 1
  fi

  exit 0
else
  usage
fi
