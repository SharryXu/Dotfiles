#!/bin/bash

# Decide if the input string can be represented as a number.
# Globals:
# Arguments:
#   String
# Returns:
#   None

function usage() {
  echo "Usage: $(basename $0) input_string" 1>&2
  exit 1
}

if [ $# -eq 1 ]; then
  if [ "$1" -gt 0 ] 2>/dev/null ;then
    echo 0
  else
    echo 1
  fi
else
  usage
fi
